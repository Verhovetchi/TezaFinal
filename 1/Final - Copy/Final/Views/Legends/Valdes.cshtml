                              <th width="5%"><img alt="" class="img-responsive  emblpadd3" src="https://barcaman.ru/media/bearleague/thumb/Soccer_GOAL_PENALTY_GOAL_1.png" width="24" title="Post-matches kicked penalty"></th>
                              <th width="5%"><img alt="" class="img-responsive  emblpadd3" src="https://barcaman.ru/media/bearleague/thumb/Soccer_PENALTY_MISS.png" width="24" title="Post-matches not kicked penalty"></th>
                              <th width="5%"><img alt="" class="img-responsive  emblpadd3" src="https://barcaman.ru/media/bearleague/thumb/Soccer_YELLOW_CARD_3.png" width="24" title="Yellow card"></th>
                              <th width="5%"><img alt="" class="img-responsive  emblpadd3" src="https://barcaman.ru/media/bearleague/thumb/Soccer_RED_CARD.png" width="24" title="Red card"></th>
                              <th width="5%"><img alt="" class="img-responsive  emblpadd3" src="https://barcaman.ru/media/bearleague/thumb/Soccer_RED_YELLOW_CARD.png" width="24" title="Second yellow card"></th>
                         </tr>
                    </thead>
                    <tbody>
                         @foreach (var item in ViewBag.Championships)
                         {
                              <tr>
                                   <td>@item</td>
                                   <td><img alt="" class="img-thumbnail img-responsive emblInline" src="https://barcaman.ru/media/bearleague/thumb/bl15179076121892.png" width="40">Barcelona</td>
                                   @for (int k = 0; k < 15; k++)
                                   {
                                        <td>@ViewBag.Goals[j, k]</td>
                                   }

                                   <span style="display:none">@(j = j + 1);</span>
                              </tr>
                         }
                    </tbody>
               </table>
